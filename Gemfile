
source "https://rubygems.org"

# GitHub Pages bundles Jekyll and most plugins (feed, sitemap, redirect-from, jemoji, etc.)
gem "github-pages", group: :jekyll_plugins

# Needed by Minimal Mistakes to speed up includes
gem "jekyll-include-cache", group: :jekyll_plugins

# Helpful for local `bundle exec jekyll serve` when using remote_theme
gem "jekyll-remote-theme", group: :jekyll_plugins

# Only for local serve on Ruby 3.x
gem "webrick", "~> 1.8"
